import java.util.Scanner;


import java.util.Scanner;

    public class java7{
        public static void main(String[] args) {
            Scanner input = new Scanner(System.in);
            int count =1;
            while(true){
                char p = input.next().charAt(0);
                System.out.println("input "+count+": "+p);
                count++;
                if (p =='A') {
                    break;
                }
            }
        }
    }

