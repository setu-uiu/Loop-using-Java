import java.util.Scanner;
public class java2 {
    public static void main(String args[])
    {
        Scanner input =new Scanner(System.in);
        int x=input.nextInt();
        for(int i=1;i<=2*x-1;i+=2)
        {
            System.out.print(i);
            if(i<2*x-1)
            {
                System.out.printf(",");
            }
        }
    }
}
