import java.util.Scanner;

public class java8 {
    public static void main(String[] args) {
        Scanner setu = new Scanner(System.in);
        System.out.println("Enter the number : ");
        int num = setu.nextInt();
        int reverse_num =0;
        int digit;
        while (num != 0){
            digit = num % 10;
            reverse_num = reverse_num * 10 +digit;
            num /=10;
        }
        System.out.println("Answer : "+reverse_num);
    }
}
